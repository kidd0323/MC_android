package com.client.entity;

public class LoginPackage {
	
	private String ErrorMessage;
	private String userID;
	private String nickname;
	//Add by Bryan 20120226
	private String campus;
	private String campusID;
	private String region;
	private String regionID;
	private String sessionID;
	private String smallPhotoUrl;
	private String middlePhotoUrl;
	private String bigPhotoUrl;

	public LoginPackage(String errorMessage, String userID, String nickname,
			String campus, String campusID, String region, String regionID,
			String sessionID, String smallPhotoUrl, String middlePhotoUrl,
			String bigPhotoUrl) {
		super();
		ErrorMessage = errorMessage;
		this.userID = userID;
		this.nickname = nickname;
		this.campus = campus;
		this.campusID = campusID;
		this.region = region;
		this.regionID = regionID;
		this.sessionID = sessionID;
		this.smallPhotoUrl = smallPhotoUrl;
		this.middlePhotoUrl = middlePhotoUrl;
		this.bigPhotoUrl = bigPhotoUrl;
	}
	public LoginPackage() {
		super();
	}
	public String getSmallPhotoUrl() {
		return smallPhotoUrl;
	}
	public void setSmallPhotoUrl(String smallPhotoUrl) {
		this.smallPhotoUrl = smallPhotoUrl;
	}
	public String getMiddlePhotoUrl() {
		return middlePhotoUrl;
	}
	public void setMiddlePhotoUrl(String middlePhotoUrl) {
		this.middlePhotoUrl = middlePhotoUrl;
	}
	public String getBigPhotoUrl() {
		return bigPhotoUrl;
	}
	public void setBigPhotoUrl(String bigPhotoUrl) {
		this.bigPhotoUrl = bigPhotoUrl;
	}
	public String getNickname() {
		return nickname;
	}
	public void setNickname(String nickname) {
		this.nickname = nickname;
	}
	public String getErrorMessage() {
		return ErrorMessage;
	}
	public void setErrorMessage(String errorMessage) {
		ErrorMessage = errorMessage;
	}
	public String getUserID() {
		return userID;
	}
	public void setUserID(String userID) {
		this.userID = userID;
	}
	
	public String getSessionID() {
		return this.sessionID;
	}
	
	public String getCampus() {
		return this.campus;
	}
	
	public String getCampusID() {
		return this.campusID;
	}
	
	public String getRegion() {
		return this.region;
	}
	
	public String getRegionID() {
		return this.regionID;
	}
	
	public void setCampus(String campus) {
		this.campus = campus;
	}
	
	public void setCampusID(String campusID) {
		this.campusID = campusID;
	}
	
	public void setRegion(String region) {
		this.region = region;
	}
	
	public void setRegionID(String regionID) {
		this.regionID = regionID;
	}
	
	public void setSessionID(String sessionID) {
		this.sessionID = sessionID;
	}
}
