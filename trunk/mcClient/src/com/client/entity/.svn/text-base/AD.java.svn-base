package com.client.entity;

import java.util.ArrayList;

public class AD {
	
	private int ADNum;
	private ArrayList<String> ADList;
	private ArrayList<String> ADUrlList;
	private String ErrorMessage;
	
	
	public AD() {
		super();
		ErrorMessage = "empty";
	}
	
	


	public AD(int aDNum, ArrayList<String> aDList, ArrayList<String> aDUrlList,
			String errorMessage) {
		super();
		ADNum = aDNum;
		ADList = aDList;
		ADUrlList = aDUrlList;
		ErrorMessage = errorMessage;
	}




	public int getADNum() {
		return ADNum;
	}


	public void setADNum(int aDNum) {
		ADNum = aDNum;
	}


	public ArrayList<String> getADList() {
		return ADList;
	}


	public void setADList(ArrayList<String> aDList) {
		ADList = aDList;
	}


	public ArrayList<String> getADUrlList() {
		return ADUrlList;
	}


	public void setADUrlList(ArrayList<String> aDUrlList) {
		ADUrlList = aDUrlList;
	}


	public String getErrorMessage() {
		return ErrorMessage;
	}


	public void setErrorMessage(String errorMessage) {
		ErrorMessage = errorMessage;
	}
	
	
}
