//	}


	public int getForwardNum() {
		return forwardNum;
	}


	public void setForwardNum(int forwardNum) {
		this.forwardNum = forwardNum;
	}


	public int getCommentNum() {
		return commentNum;
	}


	public void setCommentNum(int commentNum) {
		this.commentNum = commentNum;
	}



}
